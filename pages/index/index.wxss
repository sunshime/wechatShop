/**index.wxss**/

.toppages {
  height: 65rpx;
  display: flex;
  width: 92%;
  justify-content: space-between;
  flex-wrap: nowrap;
  padding-left: 30rpx;
  padding-right: 50rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background-color: #fff;
}

.addr {
  font-size: 24rpx;
  line-height: 65rpx;
  width: 600rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #666;
}

.icon {
  width: 28rpx;
  height: 28rpx;
  margin-left: 30rpx;
}

.icon image {
  width: 100%;
  height: 100%;
  margin-bottom: -2rpx;
}

.swiper {
  width: 100%;
  height: 417rpx;
  margin-top: 65rpx;
}

.swiper-item {
  width: 100%;
  height: 100%;
}

.swiper-item image {
  width: 100%;
  height: 100%;
}

.nav {
  padding: 20rpx 0;
  display: flex;
  justify-content: space-around;
  background-color: #fff;
}

.nav text {
  font-size: 28rpx;
  color: #333;
}

.nav .img {
  height: 58rpx;
  width: 58rpx;
}

.nav .img image {
  width: 100%;
  height: 100%;
}

.brand {
  margin-top: 20rpx;
  background-color: #fff;
  width: 100%;
}

.brand .title {
  display: block;
  padding: 40rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.brand .br-body {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 12rpx;
}

.br-body .br-item {
  width: 360rpx;
  height: 235rpx;
  position: relative;
  margin-bottom: 11rpx;
}

.br-item image {
  width: 100%;
  height: 100%;
}

.br-item .texts {
  position: absolute;
  top: 10rpx;
  left: 10rpx;
}

.texts text {
  display: block;
  font-size: 28rpx;
  color: #333;
}

.new {
  margin: 20rpx 0;
  height: 210rpx;
  background-color: rgb(241, 247, 253);
  text-align: center;
  padding-top: 50rpx;
}

.new text {
  color: #8c9bae;
  font-size: 32rpx;
}

.new .see-all {
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 27rpx auto 0 auto;
  font-size: 22rpx;
  background: #d8e4f0;
  color: #8c9bae;
}

.new-goods {
  margin-top: 20rpx;
  background: #fff;
  padding-bottom: 10rpx;
  font-size: 28rpx;
  color: #333;
}

.new-goods .scroll-view {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-item {
  width: 280rpx;
  height: 416rpx;
  margin: 5rpx 0 5rpx 25rpx;
  display: inline-block;
  background-color: #fff;
}

.scroll-view-item:nth-child(n+2) {
  border-left: 1rpx solid #f4f4f4;
}

.scroll-view-item .img {
  width: 280rpx;
  height: 280rpx;
}

.scroll-view-item .img image {
  width: 100%;
  height: 100%;
  display: inline-block;
  overflow: hidden;
}

.scroll-view-item .text {
}

.scroll-view-item .text text {
  margin-top: 8rpx;
  display: block;
  width: 94%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-indent: 1em;
}

.scroll-view-item .name {
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
}

.scroll-view-item .brief {
  color: #8a8a8a;
  font-size: 24rpx;
}

.scroll-view-item .price {
  color: #9c3232;
  font-size: 24rpx;
}

.good {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.good text {
  color: #b1a279;
}

.good .see-all {
  background: #f4e9cb;
  color: #b1a279;
}

.feature {
  margin-top: 20rpx;
  width: 100%;
  background-color: #fff;
}

.feature .title {
  text-align: center;
  padding: 36rpx;
  vertical-align: middle;
}

.feature .title text {
  font-size: 28rpx;
  color: #333;
}

.feature .title .icon {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  margin-left: 5rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  vertical-align: middle;
}

.ft-body {
  width: 100%;
  background-color: #fff;
}

.ft-body .scroll-views {
  display: block;
  width: 100%;
  white-space: nowrap;
}

.scroll-views .scroll-view-items {
  display: inline-block;
  width: 575rpx;
  margin-left: 25rpx;
  font-size: 28rpx;
  color: #333;
}

.scroll-views .scroll-view-items:last-child {
  margin-right: 25rpx;
}

.scroll-views .scroll-view-items .img {
  display: block;
  width: 575rpx;
  height: 325rpx;
  border-radius: 10rpx;
}

.scroll-view-items .img image {
  width: 100%;
  height: 100%;
}

.scroll-view-items .title-text {
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-top: 8rpx;
}

.scroll-view-items .title-text .name {
  width: 90%;
  font-size: 30rpx;
  font-weight: bold;
}

.scroll-view-items .title-text .price {
  margin-top: 8rpx;
  color: #9c3232;
  font-size: 24rpx;
}

.scroll-view-items .subtitle {
  display: block;
  width: 90%;
  color: #8a8a8a;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
}

.categorys {
  margin-top: 20rpx;
}

.cates .cates-title {
  padding: 20rpx 0;
  text-align: center;
  display: block;
  color: #333;
  font-size: 28rpx;
}

.cates-body {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 10rpx;
}

.cates-body .items {
  width: 360rpx;
  background: #fff;
  margin-bottom: 10rpx;
  padding-bottom: 10rpx;
}

.cates-body .items-img {
  display: block;
  width: 302rpx;
  height: 302rpx;
  margin: 0 auto;
}

.items-img image {
  width: 100%;
  height: 100%;
}

.items-text {
}

.items-text text {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 98%;
  margin-bottom: 5rpx;
  text-indent: 1em;
  font-size: 28rpx;
  color: #333;
}

.items-text text:last-child {
  color: #9c3232;
}

.items .titles {
  line-height: 33rpx;
  color: #333;
  font-size: 33rpx;
  text-align: center;
  display: block;
  padding-top: 100rpx;
}

.items .more-img {
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}

.more-img image {
  width: 100%;
  height: 100%;
}




